    "import turtle\n",
    "t = turtle.Turtle()\n",
    "\n",
    "a = 100;\n",
    "b = 150;\n",
    "for i in range(2):\n",
    "    t.fd(a)\n",
    "    t.lt(90)\n",
    "    t.fd(b)\n",
    "    t.lt(90)\n",
    "\n",
    "turtle.done()"
